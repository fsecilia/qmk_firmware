{
    "keyboard_name": "Keychron Q1 Max",
    "manufacturer": "Keychron",
    "url": "https://github.com/Keychron",
    "maintainer": "lokher",
    "processor": "STM32F401",
    "bootloader": "stm32-dfu",
    "usb": {
        "force_nkro": true,
        "vid": "0x3434"
    },
    "features": {
        "bootmagic": true,
        "caps_word": true,
        "command": false,
        "console": false,
        "dip_switch": true,
        "encoder_map": true,
        "encoder": true,
        "extrakey": true,
        "mousekey": false,
        "nkro": true,
        "raw": true,
        "repeat_key": true,
        "rgb_matrix": true,
        "send_string": true
    },
    "build": {
        "debounce_type": "sym_eager_pk",
        "lto": true
    },
    "debounce": 20,
    "dynamic_keymap": {
        "layer_count": 6
    },
    "caps_word": {
        "double_tap_shift_turns_on": true,
        "enabled": true
    },
    "oneshot": {
        "tap_toggle": 0
    },
    "tapping": {
        "hold_on_other_key_press": true,
        "permissive_hold": true,
        "toggle": 0
    },
    "matrix_pins": {
        "cols": ["C7", "C8", "A14", "A15", "C10", "C11", "C13", "C14", "C15", "C0", "C1", "C2", "C3", "A0", "A1"],
        "rows": ["C12", "D2", "B3", "B4", "B5", "B6"]
    },
    "diode_direction": "ROW2COL",
    "encoder": {
        "rotary": [
            {
                "pin_a": "B14",
                "pin_b": "B15"
            }
        ]
    },
    "dip_switch" :{
        "pins": ["B12"]
    },
    "rgb_matrix": {
        "driver": "snled27351_spi",
        "sleep": true,
        "animations": {
            "band_spiral_val": true,
            "breathing": true,
            "cycle_all": true,
            "cycle_left_right": true,
            "cycle_out_in": true,
            "cycle_out_in_dual": true,
            "cycle_pinwheel": true,
            "cycle_spiral": true,
            "cycle_up_down": true,
            "digital_rain": true,
            "dual_beacon": true,
            "jellybean_raindrops": true,
            "pixel_rain": true,
            "rainbow_beacon": true,
            "rainbow_moving_chevron": true,
            "solid_reactive_multinexus": true,
            "solid_reactive_multiwide": true,
            "solid_reactive_simple": true,
            "solid_splash": true,
            "splash": true,
            "typing_heatmap": true
        }
    },
    "eeprom": {
        "wear_leveling": {
            "driver": "embedded_flash",
            "logical_size": 2048,
            "backing_size": 4096
        }
    }
}
